<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prof.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Linear"></activity>
        <activity android:name=".Relative"></activity>
        <activity android:name=".table"></activity>
        <activity android:name=".tab"></activity>
        <activity android:name=".list"></activity>
        <activity android:name=".CustomListView"></activity>
        <activity android:name=".grid"></activity>

    </application>

</manifest>